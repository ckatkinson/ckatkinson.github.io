      RowBox[{
       RowBox[{
        RowBox[{"x", "[", "0", "]"}], "=", "a"}], ";", 
       RowBox[{
        RowBox[{"x", "[", "n", "]"}], "=", 
        RowBox[{
         RowBox[{"x", "[", "0", "]"}], "+", 
         RowBox[{"n", "*", "h"}]}]}], ";", 
       RowBox[{
        RowBox[{"y", "[", "n", "]"}], "=", 
        RowBox[{
