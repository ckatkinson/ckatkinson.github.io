          RowBox[{"n", "-", "1"}], "]"}], "+", 
         RowBox[{"h", " ", 
          RowBox[{"f", "[", 
           RowBox[{
            RowBox[{"x", "[", 
             RowBox[{"n", "-", "1"}], "]"}], ",", 
            RowBox[{"y", "[", 
             RowBox[{"n", "-", "1"}], "]"}]}], "]"}]}]}]}]}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "1", ",", "steps"}], "}"}]}], "]"}], ";", 
