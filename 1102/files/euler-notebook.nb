 RowBox[{
  RowBox[{"euPlot", "[", 
   RowBox[{"f_", ",", "a_", ",", "b_", ",", "h_", ",", "steps_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "[", "0", "]"}], "=", "a"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"y", "[", "0", "]"}], "=", "b"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"x", "[", "0", "]"}], "=", "a"}], ";", 
        RowBox[{
         RowBox[{"x", "[", "n", "]"}], "=", 
         RowBox[{
          RowBox[{"x", "[", "0", "]"}], "+", 
          RowBox[{"n", "*", "h"}]}]}], ";", 
        RowBox[{
         RowBox[{"y", "[", "n", "]"}], "=", 
         RowBox[{
          RowBox[{"y", "[", 
           RowBox[{"n", "-", "1"}], "]"}], "+", 
          RowBox[{"h", " ", 
           RowBox[{"f", "[", 
            RowBox[{
             RowBox[{"x", "[", 
              RowBox[{"n", "-", "1"}], "]"}], ",", 
             RowBox[{"y", "[", 
              RowBox[{"n", "-", "1"}], "]"}]}], "]"}]}]}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", "steps"}], "}"}]}], "]"}], 
     "\[IndentingNewLine]", 
     RowBox[{"data", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "[", "n", "]"}], ",", 
          RowBox[{"y", "[", "n", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "0", ",", "steps"}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"dfield", " ", "=", " ", 
     RowBox[{"VectorPlot", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"f", "[", 
          RowBox[{"x", ",", "y"}], "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"a", "-", "2"}], ",", 
         RowBox[{"a", "+", "2"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"y", ",", 
         RowBox[{"b", "-", "4"}], ",", 
         RowBox[{"b", "+", "4"}]}], "}"}], ",", 
       RowBox[{"VectorScale", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"Tiny", ",", "Tiny", ",", "None"}], "}"}]}], ",", 
       RowBox[{"Axes", "\[Rule]", "Automatic"}], ",", 
       RowBox[{"StreamPoints", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b"}], "}"}], "}"}]}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"euler", " ", "=", " ", 
     RowBox[{"ListPlot", "[", 
      RowBox[{"data", ",", 
       RowBox[{"PlotStyle", "\[Rule]", 
        RowBox[{"{", "Red", "}"}]}]}], "]"}]}], ";", 
    RowBox[{"Clear", "[", 
     RowBox[{"x", ",", "y", ",", "data"}], "]"}], ";", "\[IndentingNewLine]", 
    
    RowBox[{"Show", "[", 
     RowBox[{"dfield", ",", "euler"}], "]"}]}], 
   ")"}]}], "\[IndentingNewLine]", 
 RowBox[{
