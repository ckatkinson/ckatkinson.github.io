          RowBox[{"x", ",", "c", ",", "d"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"y", ",", "c", ",", "d"}], "}"}]}], "]"}], ",", 
       RowBox[{"ParametricPlot3D", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"a", ",", "y", ",", 
           RowBox[{"f", "[", 
            RowBox[{"a", ",", "y"}], "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"y", ",", "c", ",", "d"}], "}"}], ",", 
