           RowBox[{"Thick", ",", "Blue"}], "}"}]}]}], "]"}]}], "}"}], "]"}], 
    ",", 
    RowBox[{"{", 
     RowBox[{"a", ",", "c", ",", "d"}], "}"}], ",", 
