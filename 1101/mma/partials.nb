Cell[BoxData[
 RowBox[{
  RowBox[{"traces", "[", 
   RowBox[{"f_", ",", "c_", ",", "d_"}], "]"}], ":=", 
  RowBox[{"Manipulate", "[", 
   RowBox[{
    RowBox[{"Show", "[", 
     RowBox[{"{", 
