Cell[CellGroupData[{

Cell["Defining a Piecewise Function", "Section"],

Cell[TextData[{
 "Here is how you define a piecewise function in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ":"
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], " ", "=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"x", "<", "0"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"x", "^", "2"}], ",", 
       RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", "2"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"x", "-", "4"}], ",", 
       RowBox[{"x", ">", "2"}]}], "}"}]}], "}"}], "]"}]}]], "Input",
 CellLabel->"In[21]:="],

